// Program to print the following pattern:
//     5
//    45
//   345
//  2345
// 12345

#include <stdio.h>

int main() {
    int rows = 5;

    for (int i = 0; i < rows; i++) {
        // Print spaces
        for (int j = 0; j < rows - i - 1; j++) {
            printf(" ");
        }
        // Print numbers
        for (int k = rows - i; k <= 5; k++) {
            printf("%d", k);
        }
        printf("\n");
    }

    return 0;
}
