// Program to print all the prime numbers from 1 to n.

#include <stdio.h>

int main() {
    int n, isPrime;

    printf("Enter a number: ");
    scanf("%d", &n);

    printf("Prime numbers from 1 to %d are: ", n);

    for (int i = 2; i <= n; i++) {
        isPrime = 1; // assume i is prime

        for (int j = 2; j * j <= i; j++) {
            if (i % j == 0) {
                isPrime = 0;
                break;
            }
        }

        if (isPrime) {
            printf("%d ", i);
        }
    }

    printf("\n");
    return 0;
}
